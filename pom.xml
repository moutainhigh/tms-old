			<plugin>
				<groupId>org.apache.maven.plugins</groupId>
				<artifactId>maven-war-plugin</artifactId>
				<version>2.6</version>
				<configuration>
					<archiveClasses>false</archiveClasses>
					<warSourceDirectory>src/main/webapp</warSourceDirectory>
					<warName>tms-webapp</warName>
